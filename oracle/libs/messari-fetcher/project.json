{
  "root": "libs/messari-fetcher",
  "sourceRoot": "libs/messari-fetcher/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/messari-fetcher",
        "tsConfig": "libs/messari-fetcher/tsconfig.lib.json",
        "packageJson": "libs/messari-fetcher/package.json",
        "main": "libs/messari-fetcher/src/index.ts",
        "generatePackageJson": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/messari-fetcher/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/messari-fetcher"],
      "options": {
        "jestConfig": "libs/messari-fetcher/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
