{
  "root": "libs/maintainer",
  "sourceRoot": "libs/maintainer/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/maintainer",
        "tsConfig": "libs/maintainer/tsconfig.lib.json",
        "packageJson": "libs/maintainer/package.json",
        "main": "libs/maintainer/src/index.ts",
        "generatePackageJson": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/maintainer/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/maintainer"],
      "options": {
        "jestConfig": "libs/maintainer/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
